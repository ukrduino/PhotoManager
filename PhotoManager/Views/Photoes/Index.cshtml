@model IEnumerable<PhotoManager.Models.PhotoModel>

@{
    ViewBag.Title = "Index";
}

<h2>PHOTOES</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Title)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TakenDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CreatedDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Place)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Camera)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FocalLength)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Diaphragm)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CameraLockSpeed)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ISO)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UsedFlash)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Title)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TakenDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CreatedDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Place)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Camera)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FocalLength)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Diaphragm)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CameraLockSpeed)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ISO)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UsedFlash)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
